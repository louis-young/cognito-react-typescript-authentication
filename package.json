{
  "name": "react-typescript-amazon-cognito-authentication",
  "description": "A React and TypeScript application with Amazon Cognito authentication.",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "typecheck": "tsc",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx}\"",
    "lint:write": "npm run lint -- --fix",
    "lint:ls": "ls-lint",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll",
    "format": "prettier --check \"./**/*.{js,jsx,ts,tsx,html,json,md}\"",
    "format:write": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,json,md}\"",
    "build:development": "NODE_ENV=development react-scripts build",
    "build:production": "NODE_ENV=production react-scripts build",
    "prepare": "husky install",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.1.0",
    "@aws-amplify/core": "^5.0.6",
    "@tanstack/react-query": "^4.24.9",
    "classnames": "^2.3.2",
    "react": "^18.0.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-router-dom": "^6.4.5",
    "react-spinners": "^0.13.7",
    "rooks": "^7.4.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^16.18.8",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
